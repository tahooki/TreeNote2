
<header style="height: 350px; float: left; width: 100%;">
			<div class="row" style="padding-top: 7px; padding-left: 20px; padding-right: 10px; padding-bottom: 10px; margin: 0px;font-size:14px;height: 100%;">
				<div class="col-sm-4 col-md-6" style="width: 32%; border: solid; margin-right: 13px; height: 100%; padding-top: 10px;float:left">
					<input type="text" name="userNo" value="{{profil.userNo}}" style="display:none">
					<img id="profilImg" class="img-responsive" src="{{#checkPhoto profil.photo}}{{/checkPhoto}}" alt="프로필 이미지" data-toggle="tooltip" title="수정을 원하시면 클릭해주세요!" style="cursor:pointer; height: 150px;">
					<div class="intro-text">
						<span id="proName" class="name">{{#if profil.name}}{{profil.name}}{{else}}이 름{{/if}}</span> 
						<span class="skills">{{#if profil.email}}{{profil.email}}{{else}}이메일{{/if}}</span><br>
					</div>
					
				</div>
				<div class="col-sm-4 col-md-6"
					style="width: 32%; border: solid; margin-right: 13px; height: 100%; padding: 0 0 0 0;float:left">
					<div class="panel-body"	style="height: 91%; overflow: auto; padding-bottom: 0px; margin-top: 15px; padding-top: 0px">
						<div id="keywordList" class="list-group" style="margin-bottom: 0px">
							{{#each keyword}}
							<div class="list-group-item keyword-list" style="color:black;cursor:pointer" >{{keyword}}
								<input value="{{key}}" hidden>
							</div> 
							{{/each}}							
						</div>
					</div>
				</div>
				<div class="col-sm-4 col-md-6"
					style="width: 32%; border: solid; height: 100%; padding: 0 0 0 0;float:left">
					<div class="panel-body"	style="height: 91%; overflow: auto; padding-bottom: 0px; margin-top: 15px; padding-top: 0px;">
						<div id="profilFriend" class="list-group" style="margin-bottom: 0px">
							{{#each friend}}
							<div  class="list-group-item " href="" style="margin:0px;border:0px;{{#checkStatus2 requestStatus}} {{/checkStatus2}};color: #000000;text-align: left">
								<div class="pull-left"> 
									<input type="text" class="userNoLink" value="{{userNo}}" hidden>
									<img class="img-avatar" src="{{#checkPhoto photo}}{{/checkPhoto}}"  alt="" style="width:40px; height:40px; cursor:pointer">
								</div>
							     <div class="pull-right" >
							        <img class="img-avatar deletFriend" src="../resources/img/minus3.png"  alt="" style="width:40px; height:40px; cursor:pointer">
							    </div>
							    <div class="media-body" >
							        <div class="list-group-item-heading" style="margin-left:7px">{{name}} </div>
							        <small class="list-group-item-text c-gray" style="margin-left:7px">{{email}}</small>
							    </div>
							</div>
							
							{{/each}}							
							
							
						</div>
					</div>
				</div>
			</div>
	<div class="scroll-top page-scroll ">
		<a class="btn btn-primary" href="#page-top"> 
			<img class="profilNavi" src="resources/img/btn_treelist_right.png" >
		</a>
	</div>
	</header>


	<section id="portfolio" style="height: 350px;display:none">
	<div class="scroll-left page-scroll"  style="display:none;">
		<a class="btn btn-primary" href="#page-top">
			<img class="profilNavi" src="resources/img/btn_treelist_left.png" >
		 </a>
	</div>
		<div class="container1">
			
			<div class="row" style="height: 81%;margin-top: 28px;">
				
			      <div id="demo" style="height:100%;">
			        <div class="container1">
			          <div class="row" style="height:100%">
			            <div class="span12" style="height:100%">
			
			              <div id="owl-demo" class="owl-carousel" style="height:100%">
			                <!-- 내 트리리스트 들어갈 장소 -->
							<div class="item" style="height:100%;">
								<div class="protreetitle" style="position: absolute;width: 100%;z-index: 100;text-align: center;margin-top:0px; font-family:Jeju Gothic; font-size:25px; color:white;">{{Tree.title}}</div>
								<div class="col-sm-4 portfolio-item item ">
									<input type="text" value="{{Tree.treeNo}}" hidden>
									<a href="#portfolioModal4" class="portfolio-link" data-toggle="modal">
										<div class="caption">
											<div class="caption-content" id="myDiagramMain" style="height: 100%; top: 10px"></div>
										</div> 
										<img src="resources/img/minitree.jpeg" class="img-responsive1" alt="">
									</a>
								</div>
							</div>
							{{#each subTree}}
							<div class="item" style="height:100%;">
								<div class="protreetitle" style="position: absolute;width: 100%;z-index: 100;text-align: center;margin-top:0px; font-family:Jeju Gothic; font-size:25px; color:white;">{{title}}</div>
								<div class="col-sm-4 portfolio-item item ">
									<input type="text" value="{{treeNo}}" hidden>
									<a href="#portfolioModal4" class="portfolio-link" data-toggle="modal">
										<div class="caption">
											<div class="caption-content" id="myDiagram{{@index}}" style="height: 100%; top: 10px"></div>
										</div> 
										<img src="resources/img/minitree.jpeg" class="img-responsive1" alt="">
									</a>
								</div>
							</div>
							{{/each}}			                
			              </div>
			            </div>
			          </div>
			        </div>
			    </div>
   			</div>

	</section>
	
	
	
	
	
	
	
	
	
